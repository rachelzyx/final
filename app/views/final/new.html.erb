<form action="/final" method="POST">

  <div class="form-group">
  <label for="name">Name of the Park</label>
  <input name="name" input type="text" class="form-control" id="name" placeholder="Enter the name of the park">
  </div>

  <div class="form-group">
  <label for="state">State of the Park</label>
  <input name="state" input type="text" class="form-control" id="state" placeholder="Which state does the park belong to?">
  </div>

  <div class="form-group">
  <label for="country">Country of the Park</label>
  <input name="country" input type="text" class="form-control" id="country" placeholder="Which country does the park belong to?">
  </div>

  <div class="form-group">
  <label for="rating">Rating of the Park</label>
  <input name="rating" input type="text" class="form-control" id="rating" placeholder="Rate the park">
  </div>

  <div class="form-group">
  <label for="summary">Summary</label>
  <input name="summary" input type="text" class="form-control" id="summary" placeholder="What's the park featured for?">
  </div>

  <button type="submit" class="btn btn-default">Submit</button>

</form>